<Ui xmlns="http://www.blizzard.com/wow/ui/">
    <Script file="MI2_FilterDropDown.lua"/>
	<DropDownToggleButton name="MI2_FilterDropDownTemplate" mixin="MI2_FilterDropDownMixin" virtual="true">
		<Layers>
			<Layer level="ARTWORK">
				<Texture parentKey="Icon" file="Interface\Minimap\Tracking\None">
					<Size x="8" y="8"/>
					<Anchors>
						<Anchor point="CENTER"/>
					</Anchors>
				</Texture>/>
			</Layer>
			<Layer level="OVERLAY">
				<Texture parentKey="IconOverlay" hidden="true">
					<Anchors>
						<Anchor point="TOPLEFT" relativeKey="$parent.Icon" y="-1"/>
						<Anchor point="BOTTOMRIGHT" relativeKey="$parent.Icon"/>
					</Anchors>
					<Color r="0.0" g="0.0" b="0.0" a="0.5"/>
				</Texture>
			</Layer>
		</Layers>
		<Frames>
			<Frame parentKey="DropDown" clampedToScreen="true" hidden="true"/>
		</Frames>
		<HighlightTexture alphaMode="ADD" file="Interface\Minimap\UI-Minimap-ZoomButton-Highlight"/>
		<Scripts>
			<OnLoad method="OnLoad"/>
			<OnEnter method="OnEnter"/>
			<OnLeave method="OnLeave"/>
			<OnMouseDown method="OnMouseDown"/>
			<OnMouseUp method="OnMouseUp"/>
		</Scripts>
	</DropDownToggleButton>
</Ui>
